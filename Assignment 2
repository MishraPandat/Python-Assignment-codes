#Q8
my_str = input("Enter a string:")    
words = my_str.split()   
words.sort()    
for word in words:  
   print(word)  





#Q9
x=complex(input("Enter  complex number:"))
if x.real>x.imag:
    print("real is greater",x.real)

else:
    print("Imaginary is greater",x.imag)





#Q10
x=float(input("Enter marks:"))
y=float(input("Enter marks:"))
b=float(input("Enter marks:"))
a=float(input("Enter marks:"))
z=float(input("Enter marks:"))

add=x+y+b+a+z
total=(add/500)*100

if total>90:
    print("Total number scored is:%f \n Grade is A"%total)

elif total>=80:
    print("Total number is:%f \n Grade is B"%total)

elif total<=79 and total>=65:
    print("Student has passed %f\n grade is C"%total)

elif total<33:
    print("Try exams again.\nFailed",total)
