#Q1
x=int(input("Enter a number:"))

if (x%2)==0:
    print("%d is even number"%x)
    
else:
    print("%d is odd number"%x)



#Q2
x=int(input("Enter a number"))

if (x%5)==0:
    print("%d is divisible"%x)
    
else:
    print("%d is not divisible"%x)




#Q3
x=int(input("Enter a number:"))

if x>0:
    print("%d is positive"%x)
    
elif x<0:
  print("%d is negative"%x)

else:
    print("%d is zero"%x)



#Q4
x=int(input("Enter number:"))
y=int(input("Enter number:"))
z=int(input("Enter number:"))

if (x>y)and(x>z):
    print("%d is greater"%x)

elif (y>x)and(y>z):
    print("%d is greater"%y)

else:
    print("%d is greater"%z)



#Q5
x=int(input("Enter the year:"))

if x%4==0 and x%100!=0:
    print("%d is leap year"%x)

else:
    print("%d is not a leap year"%x)




#Q6
x=int(input("Enter the number of month:"))

if (x==1 or x==3 or x==5 or x==7 or x==8 or x==10 or x==12):
    print("Number of days in %d are 31"%x)

elif (x==2):
    print("Number of days in %d is 28/29"%x)

elif (x==4 or x==6 or x==9 or x==11):
    print("Number of days in %d is 30"%x)

else:
    print("Sorry wrong input"%x)



